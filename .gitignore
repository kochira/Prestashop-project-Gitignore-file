#------------------------------------------------------------------------
# This is a template .gitignore file for git-managed Prestashop projects.
# (Github @kochira)
#------------------------------------------------------------------------

#------------------------------------
    #Ignore ds_store
#------------------------------------
*.DS_Store

#------------------------------------
    # Ignore everything in the root except modules, themes and override.
#------------------------------------
/*
!modules/
!themes/
!override/
!.gitignore
#------------------------------------
    # Ignore everything in the "plugins" directory, except the plugins you
    # specify (see the commented-out examples for hints on how to do this.)
#------------------------------------
modules/*
# !modules/my-directory-plugin/
!modules/my-directory-plugin/
!modules/my-directory-plugin/
!modules/my-directory-plugin/

#------------------------------------
    # Ignore everything in the "themes" directory, except the themes you
    # specify (see the commented-out example for a hint on how to do this.)
#------------------------------------
themes/*
!themes/my-theme/

#------------------------------------------------------------------------
    # Private files
    # The following files contain your database credentials and other personal data.
#------------------------------------------------------------------------
config/settings.*.php

#------------------------------------
    # Cache, temp and generated files
    # The following files are generated by PrestaShop.
#------------------------------------
/my-admin-folder/autoupgrade/
/cache/
/config/xml/*.xml
/log/
*sitemap.xml
/themes/*/cache/
/modules/*/config*.xml
themes/*/.sass-cache/
themes/*/css/

#------------------------------------
    # Site content
    # The following folders contain product images, virtual products, CSV's, etc.
#------------------------------------
my-admin-folder/backups/
my-admin-folder/export/
my-admin-folder/import/
download/
img/
upload/
